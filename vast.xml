<?xml version="1.0" encoding="UTF-8"?>
<VAST version="3.0">
  <Ad id="1">
    <InLine>

      <AdSystem>Custom</AdSystem>
      <AdTitle>My Video Ad</AdTitle>

      <Impression><![CDATA[
https://gen21-bms.com/impression
]]></Impression>

      <Creatives>
        <Creative sequence="1">
          <Linear>

            <!-- Video is 1 minute -->
            <Duration>00:01:00</Duration>

            <TrackingEvents>
              <Tracking event="start"><![CDATA[
https://gen21-bms.com/start
]]></Tracking>
              <Tracking event="firstQuartile"><![CDATA[
https://gen21-bms.com/q1
]]></Tracking>
              <Tracking event="midpoint"><![CDATA[
https://gen21-bms.com/q2
]]></Tracking>
              <Tracking event="thirdQuartile"><![CDATA[
https://gen21-bms.com/q3
]]></Tracking>
              <Tracking event="complete"><![CDATA[
https://gen21-bms.com/complete
]]></Tracking>
            </TrackingEvents>

            <VideoClicks>
              <ClickThrough><![CDATA[
https://gen21-bms.com
]]></ClickThrough>
            </VideoClicks>

            <MediaFiles>
              <MediaFile
                delivery="progressive"
                type="video/mp4"
                width="640"
                height="480"
                scalable="true"
                maintainAspectRatio="true">
<![CDATA[
https://storage.googleapis.com/gvabox/media/samples/stock.mp4
]]>
              </MediaFile>
            </MediaFiles>

          </Linear>
        </Creative>
      </Creatives>

    </InLine>
  </Ad>
</VAST>
